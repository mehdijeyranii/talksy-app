{
    "compilerOptions": {
        "target": "ES2016",
        "jsx": "preserve",
        "module": "CommonJS",
        "allowJs": true,
        "outDir": "./dist",
        "rootDir": "./src",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist", "client"]
}
